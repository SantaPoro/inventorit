{
	"name": "@inventorit/frontend",
	"version": "0.0.0",
	"repository": "inventorit/frontend",
	"main": "lib/index.js",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "parcel build src/index.html",
		"build:check": "yarn --cwd ../backend generate-gql-schema && yarn generate-gql-types && tsc --noEmit",
		"clean": "rimraf .cache dist **/__generated__",
		"dev": "concurrently --kill-others \"yarn generate-gql-types:watch\" \"yarn parcel:watch\"",
		"generate-gql-types": "apollo client:codegen --config apollo.config.not-vscode.js --target typescript",
		"generate-gql-types:watch": "apollo client:codegen --config ./apollo.config.not-vscode.js --target typescript --watch",
		"lint": "eslint --ext .ts,.tsx . && yarn prettier:check",
		"lint:fix": "eslint --ext .ts,.tsx --fix . && yarn prettier:format",
		"parcel:watch": "parcel -p 3000 --hmr-port 1234 watch src/index.html",
		"prettier:format": "prettier --write --ignore-path ../../.gitignore --loglevel error \"**/*.{js,jsx,json,yml,yaml}\"",
		"prettier:check": "prettier --list-different --ignore-path ../../.gitignore \"**/*.{js,jsx,json,yml,yaml}\"",
		"test": "jest --passWithNoTests"
	},
	"dependencies": {
		"@apollo/client": "^3.0.0-beta.43",
		"antd": "^4.1.4",
		"dayjs": "^1.8.18",
		"graphql": "^14.5.4",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-is": "^16.13.1",
		"react-router-dom": "^5.1.2",
		"styled-components": "^5.1.0",
		"styled-normalize": "^8.0.7"
	},
	"devDependencies": {
		"@types/node": "^13.11.1",
		"@types/parcel-env": "^0.0.0",
		"@types/react": "^16.9.25",
		"@types/react-dom": "^16.9.5",
		"@types/react-router-dom": "^5.1.4",
		"@types/styled-components": "^5.0.1",
		"apollo": "^2.27.3",
		"concurrently": "^5.2.0",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"parcel-bundler": "^1.12.4"
	}
}
