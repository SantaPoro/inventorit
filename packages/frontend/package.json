{
	"name": "@inventorit/frontend",
	"version": "0.0.0",
	"repository": "inventorit/frontend",
	"main": "lib/index.js",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "yarn build:check && webpack --config webpack.prod.ts",
		"build:check": "yarn --cwd ../backend generate-graphql-schema && yarn relay && tsc --noEmit",
		"clean": "rimraf dist **/__generated__",
		"dev": "concurrently \"yarn relay:watch\" \"yarn webpack:watch\"",
		"lint": "eslint --ext .ts,.tsx --ignore-path ../../.gitignore . && yarn prettier:check",
		"lint:fix": "eslint --ext .ts,.tsx --ignore-path ../../.gitignore --fix . && yarn prettier:format",
		"prettier:check": "prettier --list-different --ignore-path ../../.gitignore \"**/*.{js,jsx,json,yml,yaml}\"",
		"prettier:format": "prettier --write --ignore-path ../../.gitignore --loglevel error \"**/*.{js,jsx,json,yml,yaml}\"",
		"relay": "relay-compiler",
		"relay:watch": "relay-compiler --watch",
		"test": "jest --passWithNoTests",
		"webpack:watch": "webpack-dev-server --config webpack.dev.ts"
	},
	"dependencies": {
		"farce": "^0.4.1",
		"found": "^0.5.3",
		"found-relay": "^0.8.0",
		"graphql": "^15.0.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-relay": "^9.1.0",
		"react-relay-network-modern": "^4.7.2",
		"relay-runtime": "^9.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.9.6",
		"@babel/plugin-proposal-optional-chaining": "^7.9.0",
		"@babel/preset-react": "^7.9.4",
		"@babel/preset-typescript": "^7.9.0",
		"@types/dotenv-webpack": "^1.7.0",
		"@types/html-webpack-plugin": "^3.2.2",
		"@types/react": "^16.9.35",
		"@types/react-dom": "^16.9.8",
		"@types/react-relay": "^7.0.7",
		"@types/relay-runtime": "^8.0.9",
		"@types/webpack": "^4.41.12",
		"@types/webpack-dev-server": "^3.10.1",
		"@types/webpack-merge": "^4.1.5",
		"babel-loader": "^8.1.0",
		"babel-plugin-relay": "^9.1.0",
		"concurrently": "^5.2.0",
		"dotenv-webpack": "^1.8.0",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^2.5.1",
		"fork-ts-checker-webpack-plugin": "^4.1.3",
		"html-webpack-plugin": "^4.3.0",
		"relay-compiler": "^9.1.0",
		"relay-compiler-language-typescript": "^12.0.2",
		"relay-config": "^9.1.0",
		"ts-node": "^8.10.1",
		"vscode-apollo-relay": "^1.5.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2"
	}
}
